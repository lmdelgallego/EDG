/*New Entries Markup*/
.entry-new{
	width: 100%;
	height: 62px;
	float: left;
	background-color: #FFF;
	box-sizing: border-box;
	padding: 0px 20px;
	margin-bottom: 20px;
	border-radius: 2px;
}

	.entry-new.playing{
		border: 1px solid #C5E031;
	}

	.entry-new .left{ float: left;   width: calc(100% - 50%); }
	.entry-new .right{ float: right; }

	.entry-new .player{
		float: left;
		width: 42px;
		height: 42px;
		background-color: #F3F3F3;
		border-radius: 50%;
		margin-right: 20px;
		margin-top: 10px;
		display: none;
		background-image: url(<%= asset_path 'icn/playEntry.png' %>);
		background-repeat: no-repeat;
		background-position: 16px center;
		background-size: auto 18px;
	}

		.entry-new.playing .player{
			background-image: url(<%= asset_path 'icn/pauseEntry.png' %>);
			background-color: #C0E94C;
			background-position: center center;
			background-size: auto 16px;
		}

		.entry-new.audio .player{ display: block; }



	.entry-new h1{
		color: #c5e031;
		font-size: 20px;
		margin-bottom: 15px;
		float: left;
		margin-right: 0px;
		margin-top: 20px;
		 max-width: calc(100% - 178px);
		white-space: nowrap;
		text-overflow: ellipsis;
		overflow: hidden;
		text-decoration: none;
	}

		.entry-new h1 a{ color: #c5e031; text-decoration: none;}

		.entry-new.audio h1{ max-width: calc(100% - 82px); }

	.entry-new .winner{
		float: left;
		padding: 6px 12px 6px 30px;
		color: #FFF;
		background-color: #c5e031;
		text-transform: uppercase;
		border-radius: 20px;
		font-size: 11px;
		margin-top: 18px;
		background-image: url(<%= asset_path 'icn/winner.png' %>);
		background-position: 10px center;
		background-size: auto 14px;
		background-repeat: no-repeat;
	}

	.entry-new .user-name{
		float: left;
		text-transform: uppercase;
		margin-top: 24px;
		margin-right: 30px;
	}

		.entry-new .user-name a{
			color: #595959;
			text-decoration: none;
		}

		.entry-new .user-name span{
			font-size: 10px;
			margin-right: 5px;
			opacity: 0.6;
		}

	.entry-new .share-cloud,
	.entry-new .btn-upvote{
		float: left;
		margin-top: 15px;
	}

	.entry-new .btn-upvote{
		background: #c5e031;
		color: #ffffff;
		border: 2px solid #c5e031;
		border-radius: 50px;
		margin-right: 10px;
		line-height: 31px;
		text-align: center;
		width: 65px;
		transition: all 0.2s linear;
		-moz-transition: all 0.2s linear;
		-webkit-transition: all 0.2s linear;
		-o-transition: all 0.2s linear;
	}

		.entry-new .btn-upvote:hover{
			background: #E5EFAE !important;
			border-color: #E5EFAE;
			color: #A5BC29;
			text-decoration: none;
			transition: all 0.2s linear;
			-moz-transition: all 0.2s linear;
			-webkit-transition: all 0.2s linear;
			-o-transition: all 0.2s linear;
		}

	.entry-new .btn-share{
		border: 2px solid #B9B9B9;
		border-radius: 50px;
		cursor: pointer;
		line-height: 31px;
		position: relative;
		text-align: center;
		transition: background 0.2s linear;
		-moz-transition: background 0.2s linear;
		-webkit-transition: background 0.2s linear;
		-o-transition: background 0.2s linear;
		width: 65px;
	}

	.entry-new  .btn-share span{
		position: relative;
		top: 3px;
	}





